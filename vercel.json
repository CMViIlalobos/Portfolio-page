{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "/api/index.php",
            "use": "vercel-php@0.9.0"
        },
        {
            "src": "/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(favicon\\.ico|robots\\.txt)",
            "dest": "/public/$1"
        },
        {
            "src": "/(assets|css|js|images|storage)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_KEY": "base64:aGKWx2bdGnGL8z8bOAle3xcUBS/DNPE0zAXdhcBTCJo=",
        "APP_URL": "https://${VERCEL_URL}",
        "ASSET_URL": "https://${VERCEL_URL}",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": ":memory:",
        "LOG_CHANNEL": "stderr"
    }
}
